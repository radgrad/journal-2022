	abstract = {Individualized learning plans (ILPs) are being implemented in high schools throughout the United States as strategic planning tools that help students align course plans with career aspirations and often include the development of postsecondary plans. Initial indications are that ILPs may be an important method for helping students achieve both college and career readiness. Parents, teachers, and students indicate that ILPs result in students selecting more rigorous courses, better teacher– student relationships, and positive parent–school relations. This article describes the emergence and nature of ILPs, promising practice strategies as well as challenges associated with gaining whole school buy-in, and the potential for career and vocational research.},
	number = {6},
	urldate = {2017-11-01},
	journal = {Journal of Career Development},
	author = {Solberg, V. Scott and Phelps, L. Allen and Haakenson, Kristin and Durham, Julie and Timmons, Joe},
	year = {2012},
	pages = {500--514},
}

