@techreport{connolly_why_2015,
	title = {Why {Theories} of {Change} {Matter}},
	url = {https://wcer.wisc.edu/docs/working-papers/Working_Paper_No_2015_02.pdf},
	abstract = {Mindfully or not, people are theorists of change. That is, they are theorists insofar that they engage in a mental process by which they develop ideas that allow them to explain why events ought to occur (Turner, 1982). As a way of managing the uncertainty of everyday living, people rely on personal theories, or predictive assumptions, about the best ways to achieve desired effects. Personal theories about what kinds of action will bring about desired changes and why some actions work best are but some of many forms of a person’s tacit knowledge and thus typically remain unstated. For the purposes of this paper, we work from this definition: A theory of change is a predictive assumption about the relationship between desired changes and the actions that may produce those changes. Putting it another way, “If I do x, then I expect y to
occur, and for these reasons.”},
	number = {WCER Working Paper No. 2015-2},
	institution = {Wisconsin Center for Education Research, University of Wisconsin},
	author = {Connolly, Mark and Seymour, Elaine},
	year = {2015},
}

