	author = {Tang, Cara and Tucker, Cindy S. and Servin, Christian and Geissler, Markus and Stange, Melissa},
	year = {2019},
	note = {event-place: Minneapolis, MN, USA},
	keywords = {community college, csec2017, csec2y, curriculum guidelines, cybersecurity education, two-year college},
	pages = {334--335},
}

